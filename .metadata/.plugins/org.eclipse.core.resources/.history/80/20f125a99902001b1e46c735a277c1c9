package Pilas;

public class Pila {
	
	int[] stk = new int[10];
	int tope;
	
	Pila() {
		tope = -1;
	}
	
	void push(int num) {
		
		if(tope == 9) {
			System.out.println("La Pila está llena");
		} else {
			tope++;
			stk[tope] = num;
		}
	}
	
	int pop() {
		int dato = -1;
		if(tope == -1) {
			System.out.println("La Pila está vacia");
		} else {
			dato = stk[tope];
			tope--;
		}
		return dato;
	}
}
